{
  "name": "@atxm/generator-package",
  "version": "0.20.1",
  "description": "Yeoman generator for Atom packages written in JavaScript, TypeScript or CoffeeScript.",
  "files": [
    "generators",
    "lib"
  ],
  "scripts": {
    "lint:ejs": "ejslint generators/**/*.ejs",
    "lint:js": "eslint generators/**/*.js",
    "lint": "npm-run-all --parallel lint:*",
    "postinstall": "husky install",
    "release": "np",
    "test": "npm run lint"
  },
  "keywords": [
    "yeoman-generator",
    "atom",
    "atom package"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/a-t-x-m/generator-package"
  },
  "author": "",
  "license": "MIT OR GPL-2.0",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^1.0.0",
    "change-case": "^4.1.1",
    "ejs": "^3.1.3",
    "pascal-case": "^3.1.1",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.0.4",
    "spdx-license-list": "^6.1.0",
    "terminal-link": "^2.1.1",
    "update-notifier": "^5.0.9",
    "util": "^0.12.2",
    "yeoman-generator": "^4.11.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.4.0",
    "husky": "^5.0.9",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.4.0",
    "np": "^7.0.0",
    "npm-run-all": "^4.1.5"
  },
  "lint-staged": {
    "*.ejs": "ejslint",
    "*.js": "eslint --cache --fix",
    "*.json": "jsonlint"
  }
}
